CREATE TABLE `sistemaadminloja`.`produto` (`id` INT(10) NOT NULL AUTO_INCREMENT , `Produto` VARCHAR(20) CHARACTER SET armscii8 COLLATE armscii8_general_ci NOT NULL , `Quantidade` VARCHAR(5) CHARACTER SET armscii8 COLLATE armscii8_general_ci NOT NULL , `Categoria` VARCHAR(9) CHARACTER SET armscii8 COLLATE armscii8_general_ci NOT NULL , `Vendidos` VARCHAR(10) CHARACTER SET armscii8 COLLATE armscii8_general_ci NOT NULL , `Preco` INT(10) NOT NULL , `Postado` VARCHAR(1) CHARACTER SET armscii8 COLLATE armscii8_general_ci NOT NULL , `ImgLink` VARBINARY(10) NOT NULL , PRIMARY KEY (`id`)) ENGINE = InnoDB CHARSET=armscii8 COLLATE armscii8_general_ci;
